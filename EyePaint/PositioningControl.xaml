<UserControl x:Class="EyePaint.PositioningControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Loaded="onLoaded" Unloaded="onUnloaded" KeyDown="onKeyDown" Focusable="True">
    <Grid>
        <TextBlock Text="Tryck mellanslag för att fortsätta." VerticalAlignment="Center" HorizontalAlignment="Center" />
        <Grid.Effect>
            <BlurEffect x:Name="Blur" RenderingBias="Quality"/>
        </Grid.Effect>
    </Grid>
</UserControl>
